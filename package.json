    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "style:lint": "stylelint \"./src/scss/*.scss\" --syntax=scss",
        "js:lint": "eslint ./src/js/modules/*",
        "style:fix": "stylelint \"src/scss/**/*.scss\" --fix --color",
        "js:fix": "eslint ./src/js/modules/* --fix --color"
    },
    "dependencies": {
        "jquery": "^3.4.1",
        "lazysizes": "^5.2.0",
        "owl.carousel": "^2.3.4",
        "slick-carousel": "^1.8.1"
    },
    "eslintConfig": {
        "extends": [
            "airbnb-base"
        ],
        "rules": {
            "indent": [
                "error",
                4
            ],
            "no-use-before-define": "off",
            "no-undef": "off",
            "max-len": [
                "error",
                {
                    "code": 150
                }
            ]
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
       
        "./src/js/modules/*": [
            "npm run js:fix",
            "git add"
        ]
    },
